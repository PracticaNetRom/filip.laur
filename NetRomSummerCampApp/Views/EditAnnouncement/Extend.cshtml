@model NetRomSummerCampApp.ViewModel.AnnouncementAuthViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (@Html.BeginForm("Extend", "EditAnnouncement", FormMethod.Post))
{
